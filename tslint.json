{
  "extends": [
    "@cartant/tslint-config",
    "@cartant/tslint-config-etc"
  ],
  "rules": {
    "object-literal-sort-keys": { "severity": "off" },
    "trailing-comma": { "severity": "off" },
    "typedef": {
      "options": [
        "parameter",
        "property-declaration"
      ],
      "severity": "error"
    }
  }
}
